.row(style="padding: 5px; ")
	.col-lg-12.col-sm-12.col-xs-12
		h5.row-title
			i.fa.fa-list-alt.darkorange
			| {{ votacion.nombre }}
		.row
			.col-lg-12.col-xs-12
				.well.with-header
					.header.bordered-purple Paticipantes alumnos
					#grid1.grid(ui-grid="gridOptions" ui-grid-edit external-scopes="$scope" ui-grid-resize-columns)


			.col-lg-4.col-xs-12
				.well.with-header
					.header.bordered-yellow Inscribir participantes
					

					div.btn-group(style="padding-top:2px; padding-left: 4px; ")
						a.btn.btn-default(href="" ng-click="inscribirGrupo(grupo)" 
							ng-repeat="grupo in grupos" ng-if="hasRoleOrPerm(['profesor', 'admin'])"
							ng-class="{'disabled': grupo.presionado}") {{grupo.nombre}}
					
					a.btn.btn-primary(href="" ng-click="inscribirProfesores()" 
						ng-if="hasRoleOrPerm(['profesor', 'admin'])"
						ng-class="{'disabled': btnProfPresionado }") Inscribir profesores
						


					

